  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-helpers/test-start-point.js"
    ],
    "moduleNameMapper": {
      ".+\\.(jpg|jpeg|png|gif)$": "<rootDir>/test-helpers/image-mock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "\\.(svg)$": "<rootDir>/test-helpers/mock-svg-string.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/lib/"
    ]
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:library": "rm -rf lib && cross-env BABEL_OUTPUT=commonjs babel src --out-dir lib/ --ignore 'src/**/*.spec.js','src/**/*.test.js','__mocks__/**' --copy-files",
    "start": "webpack-dev-server --mode development",
    "stop": "killall node",
    "lint": "standard --fix",
    "test": "jest",
    "test-watch": "jest --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:debug": "DEBUG=cypress:* cypress run",
    "test:all": "npm run test && npm run start & wait-on http://localhost:9001 && npm run cypress:run && npm run stop"
  },
  "publishConfig": {
    "access": "public"
  }
